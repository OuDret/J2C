    %pad/u 32;
    %pushi/vec4 2, 0, 32;
    %div;
    %cmpi/u 0, 0, 32;
    %flag_or 5, 4; GT is !LE
    %flag_inv 5;
    %jmp/0xz  T_1.6, 5;
    %load/vec4 v0x6090bcd46780_0;
