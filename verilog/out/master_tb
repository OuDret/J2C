v0x6090bcd462e0_0 .var "out", 5 0;
v0x6090bcd463c0_0 .net "rst", 0 0, v0x6090bcd46d50_0;  alias, 1 drivers
E_0x6090bcd2ed60 .event posedge, v0x6090bcd13820_0;
    .scope S_0x6090bcce8cf0;
T_0 ;
    %wait E_0x6090bcd2ed60;
    %load/vec4 v0x6090bcd463c0_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 6;
    %assign/vec4 v0x6090bcd462e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6090bcd462e0_0;
